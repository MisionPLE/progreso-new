		<script>
      		var map;
		var mark;
		var geocoder;
      		function initMap() {
			geocoder = new google.maps.Geocoder();
        			map = new google.maps.Map(document.getElementById('map'), {
          			center: {lat: -12.070, lng: -76.949},
        				  zoom: 12
       				 });
			// This event listener will call addMarker() when the map is clicked.
   			     map.addListener('click', function(event) {
    			   	   addMarker(event.latLng);
		     	   });
			mark = new google.maps.Marker({
     		     position: {lat: -12.070, lng: -76.949},
          		map: map
   			     });
    		  }
		function addMarker(location) {
		     mark.setMap(null)
     		     mark = new google.maps.Marker({
     		     position: location,
          		map: map
   			     });
			unlookup(location);
    		}
		function updateMap(res, stat) {
			if (stat == 'OK') {
				map.setCenter(res[0].geometry.location)
				addMarker(res[0].geometry.location)
			}
		}
		function updateAddr(res, stat) {
			var addr = document.getElementById("addr");
			if (stat == 'OK') {
				if (res[0]) {
					addr.value = res[0].formatted_address;
					return
				}
			}
			addr.value = "";
		}
		function lookup() {
			var addr = document.getElementById("addr").value;
			geocoder.geocode( { 'address': addr}, updateMap);
		}
		function unlookup(loc)	{
			geocoder.geocode({'location': loc}, updateAddr);
		}	
  		  </script>
